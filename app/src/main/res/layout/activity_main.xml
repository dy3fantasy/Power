<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingStart="5dp"
    android:paddingTop="5dp"
    android:paddingBottom="5dp"
    android:paddingEnd="5dp"
    tools:context="com.arib.power.MainActivity">

    <TextView
        android:id="@+id/speed"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:text="-1"
        android:textSize="75sp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/speed"
        android:layout_centerHorizontal="true"
        android:text="mph"
        android:textSize="20sp"
        android:id="@+id/textView7" />

    <TextView
        android:id="@+id/xAccel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_centerVertical="true"
        android:text="X"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/peakXAccel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/xAccel"
        android:text="0"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/peakYAccel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_below="@+id/peakXAccel"
        android:text="0"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/peakZAccel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_below="@+id/peakYAccel"
        android:text="0"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/yAccel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/xAccel"
        android:text="Y"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/zAccel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/yAccel"
        android:text="Z"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/topSpeed"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView7"
        android:layout_centerHorizontal="true"
        android:text="0" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/peakXAccel"
        android:layout_toStartOf="@+id/peakXAccel"
        android:layout_marginEnd="10dp"
        android:text="PeakXAccel:"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/peakXAccel"
        android:layout_toStartOf="@+id/peakXAccel"
        android:layout_marginEnd="10dp"
        android:text="PeakYAccel:"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/peakYAccel"
        android:layout_toStartOf="@+id/peakYAccel"
        android:layout_marginEnd="10dp"
        android:text="PeakZAccel:"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/zTlabel"
        android:layout_width="130dp"
        android:layout_height="15dp"
        android:layout_marginRight="10px"
        android:layout_marginLeft="10px"
        android:layout_above="@+id/zeroThirty"
        android:text="0-30"
        android:textAlignment="center" />

    <TextView
        android:id="@+id/zFFlabel"
        android:layout_width="130dp"
        android:layout_height="15dp"
        android:layout_toRightOf="@id/zTlabel"
        android:layout_marginRight="10px"
        android:layout_above="@+id/zeroFortyfive"
        android:text="0-45"
        android:textAlignment="center" />

    <TextView
        android:id="@+id/zSlabel"
        android:layout_width="130dp"
        android:layout_height="15dp"
        android:layout_toRightOf="@id/zFFlabel"
        android:layout_marginRight="10px"
        android:layout_above="@+id/zeroSixty"
        android:text="0-60"
        android:textAlignment="center" />

    <Chronometer
        android:id="@+id/zeroThirty"
        android:layout_width="130dp"
        android:layout_height="40dp"
        android:layout_marginRight="10px"
        android:layout_marginLeft="10px"
        android:layout_above="@+id/bestTime1"
        android:text="READY"
        android:textAlignment="center"
        android:textSize="30sp" />

    <Chronometer
        android:id="@+id/zeroFortyfive"
        android:layout_width="130dp"
        android:layout_height="40dp"
        android:layout_toRightOf="@id/zeroThirty"
        android:layout_marginRight="10px"
        android:layout_above="@+id/bestTime2"
        android:text="READY!"
        android:textAlignment="center"
        android:textSize="30sp" />

    <Chronometer
        android:id="@+id/zeroSixty"
        android:layout_width="130dp"
        android:layout_height="40dp"
        android:layout_toRightOf="@id/zeroFortyfive"
        android:layout_marginRight="10px"
        android:layout_above="@+id/bestTime3"
        android:text="READY!"
        android:textAlignment="center"
        android:textSize="30sp" />

    <TextView
        android:id="@+id/bestTime1"
        android:layout_width="130dp"
        android:layout_height="24dp"
        android:layout_above="@+id/topZero30"
        android:layout_marginLeft="10px"
        android:layout_marginRight="10px"
        android:text="Best Time"
        android:textAlignment="center"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/bestTime2"
        android:layout_width="130dp"
        android:layout_height="24dp"
        android:layout_above="@+id/topZero45"
        android:layout_toRightOf="@id/bestTime1"
        android:layout_marginRight="10px"
        android:text="Best Time"
        android:textAlignment="center"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/bestTime3"
        android:layout_width="130dp"
        android:layout_height="24dp"
        android:layout_above="@+id/topZero60"
        android:layout_toRightOf="@id/bestTime2"
        android:layout_marginRight="10px"
        android:text="Best Time"
        android:textAlignment="center"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/topZero30"
        android:layout_width="130dp"
        android:layout_height="40dp"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="2dp"
        android:layout_marginRight="10px"
        android:layout_marginLeft="10px"
        android:text="-1"
        android:textAlignment="center" />

    <TextView
        android:id="@+id/topZero45"
        android:layout_width="130dp"
        android:layout_height="40dp"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="2dp"
        android:layout_toRightOf="@id/topZero30"
        android:layout_marginRight="10px"
        android:text="-1"
        android:textAlignment="center" />

    <TextView
        android:id="@+id/topZero60"
        android:layout_width="130dp"
        android:layout_height="40dp"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="2dp"
        android:layout_toRightOf="@id/topZero45"
        android:layout_marginRight="10px"
        android:text="-1"
        android:textAlignment="center" />


</RelativeLayout>
